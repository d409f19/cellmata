world {
    size = 100 [wrap], 200 [wrap]
    tickrate = 1
    cellsize = 5
}

state ident (0, 0, 0) {
    let even = 0;
    let arr = {-1, 1, 2, 3, 4};

    for (arr[2] = 5; arr[2] >= 0; arr[2] = arr[2] - 1) {
        // If current number is even, increment counter
        if (x % 2 == 0) {
            even = even + 1;
        }
    }

    become ident;
}
