world {
    size = 100 [wrap], 200 [wrap];
    tickrate = 1;
    cellsize = 5;
}

state ident (0, 0, 0) {
    // Number of 'ident' neighbours in builtin moore neighbourhood
    let x = count(moore, ident);
    become ident;
}
