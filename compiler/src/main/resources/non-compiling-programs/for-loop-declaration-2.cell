world {
    size = 100 [wrap], 200 [wrap]
    tickrate = 1
    cellsize = 5
}

state ident (0, 0, 0) {
    let even = 0;
    
    for (let x = 5; y >= 0; y = y - 1) {
        // If current number is even, increment counter
        if (x % 2 == 0) {
            even = even + 1;
        }
    }

    become ident;
}
